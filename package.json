{
  "name": "casbin-sequelize-adapter",
  "version": "2.1.0",
  "description": "Sequelize adapter for Casbin",
  "main": "lib/adapter.js",
  "typings": "lib/adapter.d.ts",
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "yarn run lint && yarn run test && yarn build",
    "build": "rimraf lib && tsc",
    "lint": "tslint \"src/**/*.ts\"",
    "fix": "tslint \"src/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "format": "yarn format:check --write",
    "format:check": "prettier --check \"**/*.{md,json}\" \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@types/sequelize": "^4.28.9",
    "coveralls": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "mysql2": "^2.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "casbin": "^5.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.12",
    "sequelize-typescript": "^1.1.0"
  },
  "files": [
    "lib",
    "examples"
  ],
  "homepage": "https://casbin.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/node-casbin/casbin-sequelize-adapter.git"
  },
  "keywords": [
    "casbin",
    "node-casbin",
    "adapter",
    "sequelize",
    "access-control",
    "authorization",
    "auth",
    "authz",
    "acl",
    "rbac",
    "abac",
    "orm"
  ],
  "author": "Node-Casbin",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/node-casbin/sequelize-adapter/issues"
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/*.+(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
